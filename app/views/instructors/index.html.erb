<div class="album bg-light py-5 px-5 my-5">
  <h1>All Instructors</h1>
  <div class="row">
    <ol>
      <% @instructors.each do |instructor| %>
          <li> <%= link_to instructor.second_name instructor_path(instructor), class: "btn btn-sm btn-outline-secondary"%><%= instructor.first_name%> <%=instructor.second_name%> (<%= link_to "View Instructor", instructor_path(instructor), method: :get, class: "btn btn-sm btn-outline-secondary"%>)</li>
      <% end %>
    </ol>
  </div>
</div>
